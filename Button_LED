library IEEE;
use IEEE.STD_LOGIC_1164.ALL;

entity led_btn is
    Port ( btn : in  STD_LOGIC;
           led : out STD_LOGIC);
end led_btn;

architecture Behavioral of led_btn is
begin
    led <= not btn;
end Behavioral;

CST

IO_LOC "led" D7;
IO_LOC "btn" H11;
IO_PORT "btn" PULL_MODE=DOWN BANK_VCCIO=3.3;
